msgid "AFP Login"
msgstr "AFP-login"

#: /home/dmark/dev/kio-afp/src/kafp_worker.cpp:209
msgid "Please enter your username and password."
msgstr "Var god ange namn och lösenord."

#: /home/dmark/dev/kio-afp/src/kafp_worker.cpp:211
msgid "Server:"
msgstr "Server:"

#: /home/dmark/dev/kio-afp/src/kafp_worker.cpp:263
#: /home/dmark/dev/kio-afp/src/kafp_worker.cpp:285
msgid "AFP daemon not responding (retry in %1 s)"
msgstr "AFP-demonen svarar inte (försöker igen om %1 s)"

#: /home/dmark/dev/kio-afp/src/kafp_worker.cpp:360
msgid "Authentication failed. Please try again."
msgstr "Autentiseringen misslyckades. Försök igen."

#: /home/dmark/dev/kio-afp/src/kafp_worker.cpp:410
msgid "No volume specified in URL"
msgstr "Ingen volym angiven i URL"

#: /home/dmark/dev/kio-afp/src/kafp_worker.cpp:554
msgid "Volume not found: %1"
msgstr "Volymen kunde inte hittas: %1"

#: /home/dmark/dev/kio-afp/src/kafp_worker.cpp:557
msgid "AFP server not found"
msgstr "AFP-server kunde inte hittas"

#: /home/dmark/dev/kio-afp/src/kafp_worker.cpp:562
#: /home/dmark/dev/kio-afp/src/kafp_worker.cpp:594
msgid "Cannot communicate with AFP server"
msgstr "Kunde inte kommunicera med AFP-server"

#: /home/dmark/dev/kio-afp/src/kafp_worker.cpp:567
msgid "Not connected to AFP server"
msgstr "Inte ansluten till AFP-server"

#: /home/dmark/dev/kio-afp/src/kafp_worker.cpp:570
msgid "Not attached to volume"
msgstr "Inte kopplad till volym"

#: /home/dmark/dev/kio-afp/src/kafp_worker.cpp:573
#: /home/dmark/dev/kio-afp/src/kafp_worker.cpp:585
msgid "Authentication with AFP server failed"
msgstr "Autentiseringen men AFP-server misslyckades"

#: /home/dmark/dev/kio-afp/src/kafp_worker.cpp:576
msgid "AFP error %1"
msgstr "AFP-felmeddelande %1"

#: /home/dmark/dev/kio-afp/src/kafp_worker.cpp:588
msgid "Could not find AFP server"
msgstr "Kunde inte hitta AFP-server"

