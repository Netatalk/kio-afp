msgstr "Fel vid inläsning av data från klienten"

#: /home/dmark/dev/kio-afp/src/kafp_worker.cpp:941
msgid "Cannot create directory at volume level"
msgstr "Kan inte skapa katalog på volymnivå"

#: /home/dmark/dev/kio-afp/src/kafp_worker.cpp:962
msgid "Cannot delete volume root"
msgstr "Kan inte radera volymens rot"

#: /home/dmark/dev/kio-afp/src/kafp_worker.cpp:989
msgid "Cannot rename volume roots"
msgstr "Kan inte ändra namn på volymens rot"

#: /home/dmark/dev/kio-afp/src/kafp_worker.cpp:993
msgid "Cannot rename across different volumes"
msgstr "Kan inte ändra namn mellan olika volymer"

#: /home/dmark/dev/kio-afp/src/kafp_worker.cpp:1022
msgid "Cannot chmod volume root"
msgstr "Kan inte göra chmod på volymens rot"
